<div class="container-fluid">
  <div class="row-fluid">
    <div class="list-group">
      <a href="#" class="list-group-item disabled">
        日報一覧
      </a>
      <% if !@daily_items.nil? then %>
        <% @daily_items.each do |item| %>
          <a href="../daily_items/show/<%= item.id %>" class="list-group-item"><%= item.created_at.strftime("%Y/%m/%d") %></a>
        <% end %>
      <% end %>
    </div>
  </div>
</div>
